{
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "rangeStrategy": "widen",
  "regexManagers": [
    {
      "fileMatch": ["/release.*\\.yml"],
      "matchStrings": ["knope.*\\s*with:\\s*version:\\s*(?<currentValue>.*?)$"],
      "depNameTemplate": "knope",
      "datasourceTemplate": "crate"
    }
  ]
}
